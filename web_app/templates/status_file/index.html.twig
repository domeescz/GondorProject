{% extends 'base.html.twig' %}

{% block title %}StatusFile index{% endblock %}

{% block body %}
    <h1>StatusFile index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>StatusName</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for status_file in status_files %}
            <tr>
                <td>{{ status_file.id }}</td>
                <td>{{ status_file.StatusName }}</td>
                <td>
                    <a href="{{ path('app_status_file_show', {'id': status_file.id}) }}">show</a>
                    <a href="{{ path('app_status_file_edit', {'id': status_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_status_file_new') }}">Create new</a>
{% endblock %}
